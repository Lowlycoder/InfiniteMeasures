{
  "name": "colibrit",
  "version": "0.1.0",
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "prettier": "latest"
  },
  "scripts": {
    "start:dev": "./node_modules/.bin/browser-sync start --proxy 'app-g3a' --files '*'"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all",
    "tabWidth": 4,
    "overrides": [
      {
        "files": [
          "*.js",
          "*.json"
        ],
        "options": {
          "tabWidth": 2
        }
      }
    ]
  }
}
